@inherits UmbracoViewPage<Umbraco.Core.Models.Blocks.BlockListModel>

@{
    if (!Model.Any())
    {
        return;
    }

    foreach (var block in Model)
    {
        if (block?.ContentUdi == null)
        {
            continue;
        }

        var data = block.Content;
        @Html.Partial($"BlockList/Components/{data.ContentType.Alias}", block)
    }
}